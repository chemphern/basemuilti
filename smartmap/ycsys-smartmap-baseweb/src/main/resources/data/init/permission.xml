                        resource-edit,
                        resource-type-edit
                    </code>
                </func>
                <func name="删除" sort="3">
                    <code>
                        resource-delete,
                        resource-type-delete
                    </code>
                </func>
                <func name="查看" sort="4">
                    <code>
                        resource-view,
                        resource-list,
                        resource-list-data,
                        resource-type-list
                    </code>
                </func>
                <func name="移动" sort="5">
                    <code>
                        resource-move
                    </code>
                </func>
                <func name="备份" sort="6">
                	<code>
