                        service-delete
                    </code>
                </func>
                <func name="版本刷新" sort="4">
                    <code>
                        service-refresh-version
                    </code>
                </func>
                <func name="注册" sort="5">
                    <code>
                        service-register
                    </code>
                </func>
                <func name="导出" sort="6">
                    <code>
                        service-export
                    </code>
                </func>
                <func name="导入" sort="7">
                    <code>
                        service-import
                    </code>
                </func>
                <func name="查看" sort="8">
                    <code>
                        service-list,
                        service-type-list
                    </code>
                </func>
                <func name="编辑" sort="9">
