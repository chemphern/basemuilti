        <module name="安全管理" sort="1">
            <url sort="1" name="机构管理" href="/org/list" funcs="sys-org-list">
                <func name="查看" sort="1">
                    <code>
                        sys-org-list,
                        sys-org-listData
                    </code>
                </func>
                <func name="新增" sort="2">
                    <code>
                        sys-org-addOrgv,
                        sys-org-saveOrUpdate,
                        sys-org-getOrgs,
                        sys-dict-getDictItemByCode,
                        sys-area-getAreas
                    </code>
                </func>
                <func name="修改" sort="3">
                    <code>
                        sys-org-updateOrgv,
                        sys-org-saveOrUpdate,
                        sys-area-getAreas,
                        sys-dict-getDictItemByCode
                    </code>
                </func>
                <func name="删除" sort="4">
                    <code>
                        sys-org-delete
                    </code>
                </func>
                <func name="新增子机构" sort="5">
                    <code>
                        sys-org-addChildOrgv,
                        sys-area-getAreas,
                        sys-dict-getDictItemByCode,
                        sys-org-saveOrUpdate
                    </code>
                </func>
            </url>
            <url sort="2" name="用户管理" href="/user/list" funcs="sys-user-list">
                <func name="查看" sort="1" >
                    <code>
                        sys-user-list,
                        sys-user-listData,
                        sys-org-getOrgs,
                        sys-user-showUserv
                    </code>
                </func>
                <func name="新增" sort="2">
                    <code>
                        sys-user-addUserv,
                        sys-dict-getDictItemByCode,
                        sys-role-getRoles,
                        sys-user-saveOrUpdate
                    </code>
                </func>
                <func name="修改" sort="3">
                    <code>
                        sys-user-updateUserv,
                        sys-user-saveOrUpdate,
                        sys-dict-getDictItemByCode,
                        sys-role-getRoles
                    </code>
                </func>
                <func name="删除" sort="4">
                    <code>
                        sys-user-delete
                    </code>
                </func>
            </url>
            <url name="角色管理" sort="3" href="/role/list" funcs="sys-role-list">
                <func name="查看" sort="1">
                    <code>
                        sys-role-list,
                        sys-role-listData
                    </code>
                </func>
                <func name="新增" sort="2">
                    <code>
                        sys-role-addRolev,
                        sys-role-saveOrUpdate
                    </code>
                </func>
                <func name="修改" sort="3">
                    <code>
                        sys-role-updateRolev,
                        sys-role-saveOrUpdate
                    </code>
                </func>
                <func name="删除" sort="4">
                    <code>
                        sys-role-delete
                    </code>
                </func>
                <func name="分配权限" sort="5">
                      <code>
                        sys-role-roleGivev,
                        sys-permission-findPermissions,
                        sys-role-updatePermission
                    </code>
                </func>
            </url>
            <url name="权限管理" sort="4" href="/permission/list" funcs="sys-permission-list">
                <func name="查看" sort="1">
                    <code>
                        sys-permission-findSystems,
                        sys-permission-list,
                        sys-permission-listData
                    </code>
                </func>
                <func name="导入" sort="2">
                    <code>
                        sys-permission-importPermissionv,
                        sys-permission-importPermission
                    </code>
                </func>
                <func name="导出" sort="3">
                    <code>
                        sys-permission-exportPermission
                    </code>
                </func>
            </url>
            <url name="地区管理" sort="5" href="/area/list" funcs="sys-area-list">
                <func name="查看" sort="1">
                    <code>
                        sys-area-list,
                        sys-area-listData
                    </code>
                </func>
                <func name="新增" sort="2">
                    <code>
                        sys-area-addAreav,
                        sys-area-saveOrUpdate
                    </code>
                </func>
                <func name="修改" sort="3">
                    <code>
                        sys-area-updatev,
                        sys-area-saveOrUpdate
                    </code>
                </func>
                <func name="删除" sort="4">
                    <code>
                        sys-area-delete
                    </code>
                </func>
            </url>
            <url name="缓存管理" sort="6" href="/cache/list" funcs="sys-cache-list">
                <func name="管理" sort="1">
                    sys-cache-refreshDict,
                    sys-cache-listData,
                    sys-cache-list
                </func>
            </url>
        </module>
        <module sort="2" name="系统配置">
            <url sort="1" name="字典管理" href="/dictionary/list" funcs="sys-dictionary-list">
                <func name="查看" sort="1">
